<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <listener>
    <listener-class>cn.ishow.manage.listener.SessionManage</listener-class>
  </listener>

  <servlet>
    <servlet-name>DispatchServlet</servlet-name>
    <servlet-class>cn.ishow.manage.system.DispatchServlet</servlet-class>
  </servlet>

<servlet-mapping>
  <servlet-name>DispatchServlet</servlet-name>
  <url-pattern>/*</url-pattern>
</servlet-mapping>

  <session-config>
    <session-timeout>1</session-timeout>
  </session-config>

  <welcome-file-list>
    <welcome-file>system/test</welcome-file>
  </welcome-file-list>


</web-app>
